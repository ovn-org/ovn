check ovn-nbctl --wait=sb ls-add sw0
check_row_count Datapath_Binding 1
dp1=$(fetch_column Datapath_Binding _uuid external_ids:name=sw0)

# Make nbdb ovsdb-server drop connection from ovn-northd.
